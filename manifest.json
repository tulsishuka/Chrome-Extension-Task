{
  "manifest_version": 3,
  "name": "Tab Title Grabber",
  "version": "1.0",
  "description": "Get the title of the current tab and show it in popup",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
